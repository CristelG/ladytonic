generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Newsletter {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String
  surname   String
  createdAt DateTime @default(now())
}
